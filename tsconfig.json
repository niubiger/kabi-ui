{
    "compilerOptions": {
      "module": "esnext",
      "target": "es2015",
      "moduleResolution": "node",
      "strict": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": false,
      "noEmit": true,
      "jsx": "react",
      "suppressImplicitAnyIndexErrors": true,
      "noImplicitThis": false
    }
}